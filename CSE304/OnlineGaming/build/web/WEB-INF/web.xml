<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <!--jsp servlets-->
    <servlet>
        <servlet-name>ForumPage</servlet-name>
        <jsp-file>&gt;/ForumPage.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ForumReplyPage</servlet-name>
        <jsp-file>&gt;/ForumReplyPage.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>ForumPage</servlet-name>
        <url-pattern>/ForumPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForumReplyPage</servlet-name>
        <url-pattern>/ForumReplyPage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginProcess</servlet-name>
        <servlet-class>muman.servlets.LoginProcess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateAccount</servlet-name>
        <servlet-class>muman.servlets.CreateAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FlagPost</servlet-name>
        <servlet-class>muman.servlets.forum.FlagPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FlagReply</servlet-name>
        <servlet-class>muman.servlets.forum.FlagReply</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginProcess</servlet-name>
        <url-pattern>/LoginProcess.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateAccount</servlet-name>
        <url-pattern>/CreateAccount.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FlagPost</servlet-name>
        <url-pattern>/FlagPost.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FlagReply</servlet-name>
        <url-pattern>/FlagReply.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
